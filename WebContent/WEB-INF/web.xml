<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>linear_algebra</display-name>
  
  <listener>
  	<listener-class>com.ren.spring.core.ContextLoaderListener</listener-class>
  </listener>
  
  <filter>
    <filter-name>coreFilter</filter-name>
    <filter-class>com.ren.struts.core.MyDispatcherFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>coreFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  
  <welcome-file-list>
  	<welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>